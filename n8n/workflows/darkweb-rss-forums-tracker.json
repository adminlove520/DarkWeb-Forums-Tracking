{
    "name": "darkweb-rss-forums-tracker",
    "nodes": [
        {
            "parameters": {
                "rule": {
                    "interval": [
                        {
                            "field": "minutes",
                            "minutes": 30
                        }
                    ]
                }
            },
            "type": "n8n-nodes-base.scheduleTrigger",
            "typeVersion": 1.1,
            "position": [
                -200,
                0
            ],
            "id": "schedule-trigger",
            "name": "Schedule Trigger"
        },
        {
            "parameters": {
                "functionCode": "return [\n  {\n    json: {\n      rss_url: \"https://xforums.st/forums/-/index.rss\",\n      forum_name: \"XForums\"\n    }\n  }\n];"
            },
            "type": "n8n-nodes-base.function",
            "typeVersion": 1,
            "position": [
                0,
                0
            ],
            "id": "define-feeds",
            "name": "定义RSS列表"
        },
        {
            "parameters": {
                "batchSize": 1,
                "options": {}
            },
            "type": "n8n-nodes-base.splitInBatches",
            "typeVersion": 3,
            "position": [
                200,
                0
            ],
            "id": "split-feeds",
            "name": "循环处理每个订阅源"
        },
        {
            "parameters": {
                "model": "gpt-4o-mini",
                "options": {}
            },
            "type": "@n8n/n8n-nodes-langchain.lmChatOpenAi",
            "typeVersion": 1,
            "position": [
                380,
                220
            ],
            "id": "openai-model",
            "name": "OpenAI Model",
            "credentials": {
                "openAiApi": {
                    "id": "openAiApi",
                    "name": "OpenAI account"
                }
            }
        },
        {
            "parameters": {
                "sseEndpoint": "={{ $env.PLAYWRIGHT_MCP_ENDPOINT }}",
                "include": "except",
                "excludeTools": [
                    "browser_file_upload",
                    "browser_install",
                    "browser_pdf_save",
                    "browser_drag",
                    "browser_generate_playwright_test"
                ]
            },
            "type": "@n8n/n8n-nodes-langchain.mcpClientTool",
            "typeVersion": 1,
            "position": [
                540,
                220
            ],
            "id": "playwright-tool",
            "name": "Playwright MCP工具"
        },
        {
            "parameters": {
                "promptType": "define",
                "text": "={{ 'Please visit the URL: ' + $json.rss_url + '\\n\\nI need you to return the FULL, RAW XML content of this page. Use the Playwright tool to navigate to it and get the page source/content.\\n\\nIMPORTANT:\\n1. Do NOT summarize or explain.\\n2. Do NOT wrap it in markdown code blocks if possible, but if you do, I will clean it.\\n3. The output must be valid XML.' }}",
                "options": {}
            },
            "type": "@n8n/n8n-nodes-langchain.agent",
            "typeVersion": 1.7,
            "position": [
                460,
                0
            ],
            "id": "rss-fetch-agent",
            "name": "AI获取RSS内容"
        },
        {
            "parameters": {
                "functionCode": "let content = items[0].json.output || \"\";\ncontent = content.replace(/```xml/g, \"\").replace(/```/g, \"\").trim();\nconst xmlStart = content.indexOf(\"<?xml\");\nif (xmlStart > -1) {\n  content = content.substring(xmlStart);\n}\nconst rssStart = content.indexOf(\"<rss\");\nif (rssStart > -1 && (xmlStart === -1 || rssStart < xmlStart)) {\n    content = content.substring(rssStart);\n}\n\nreturn [{ json: { xml: content, rss_url: $('split-feeds').item.json.rss_url, forum_name: $('split-feeds').item.json.forum_name } }];"
            },
            "type": "n8n-nodes-base.function",
            "typeVersion": 1,
            "position": [
                800,
                0
            ],
            "id": "clean-xml",
            "name": "清洗XML文本"
        },
        {
            "parameters": {
                "mode": "json",
                "jsonPropertyName": "xml",
                "options": {}
            },
            "type": "n8n-nodes-base.xml",
            "typeVersion": 1,
            "position": [
                1000,
                0
            ],
            "id": "parse-xml",
            "name": "解析XML"
        },
        {
            "parameters": {
                "functionCode": "// Handle RSS structure variations\nlet posts = [];\ntry {\n  const channel = items[0].json.rss.channel;\n  const itemsList = channel.item || [];\n  posts = Array.isArray(itemsList) ? itemsList : [itemsList];\n} catch (e) {\n  // If parsing fails or structure is wrong, return empty to continue loop\n  posts = [];\n}\n\nreturn posts.map(post => {\n  return {\n    json: {\n      url: post.link,\n      title: post.title,\n      pubDate: post.pubDate,\n      forum_name: $('split-feeds').item.json.forum_name\n    }\n  };\n});"
            },
            "type": "n8n-nodes-base.function",
            "typeVersion": 1,
            "position": [
                1200,
                0
            ],
            "id": "format-items",
            "name": "格式化数据"
        },
        {
            "parameters": {
                "operation": "getAll",
                "tableId": {
                    "__rl": true,
                    "value": "darkweb_forums",
                    "mode": "id"
                },
                "returnAll": true,
                "filters": {
                    "conditions": [
                        {
                            "key": "post_link",
                            "value": "={{ $json.url }}",
                            "operator": "eq"
                        }
                    ]
                }
            },
            "type": "n8n-nodes-base.supabase",
            "typeVersion": 1,
            "position": [
                1400,
                0
            ],
            "id": "check-supabase",
            "name": "检查是否已存在",
            "credentials": {
                "supabaseApi": {
                    "id": "okGUlQOFiLX3NatJ",
                    "name": "Supabase account"
                }
            }
        },
        {
            "parameters": {
                "conditions": {
                    "boolean": [
                        {
                            "value1": "={{ $items(\"检查是否已存在\").length }}",
                            "value2": 0
                        }
                    ]
                }
            },
            "type": "n8n-nodes-base.if",
            "typeVersion": 1,
            "position": [
                1600,
                0
            ],
            "id": "check-if-new",
            "name": "是否为新帖子?"
        },
        {
            "parameters": {
                "workflowId": "sT0FP436BFPS5V7o",
                "mode": "each",
                "workflowInputs": {
                    "mappingMode": "defineBelow",
                    "value": {
                        "post_link": "={{ $json.url }}",
                        "forum_name": "={{ $json.forum_name }}"
                    }
                }
            },
            "type": "n8n-nodes-base.executeWorkflow",
            "typeVersion": 1,
            "position": [
                1850,
                -100
            ],
            "id": "execute-subworkflow",
            "name": "AI Agent分析"
        },
        {
            "parameters": {},
